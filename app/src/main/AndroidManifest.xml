<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mycom">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name="com.MyApplication"
        android:theme="@style/AppTheme">
        <activity android:name="com.mycom.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Demo.SlideActivity"
            android:theme="@style/MyTheme"
            android:enabled="true"
            android:exported="true"/>
        <activity android:name=".Demo.SlideLayoutActivity"
            android:theme="@style/MyTheme"
            android:enabled="true"
            android:exported="true"/>
        <activity android:name="com.fragment.single.TestSignalFragmentActivity"
            android:enabled="true"
            android:exported="true"/>


        <receiver
            android:exported="false"
            android:name="com.utils.MyBroadcastReceiver">
            <intent-filter>
                <action android:name="Fang.MyBroadcast.Error" />
                <action android:name="Fang.MyBroadcast.MSG" />
            </intent-filter>
        </receiver>
    </application>

</manifest>